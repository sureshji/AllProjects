<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		id="myDialog"
		noDataText="No Plant Found"
		title="Select Plant"
		search="handleSearch"
		confirm="handleClose"
		cancel="handleClose"
		items="{
			path : '/HT001wSet',
			sorter : {
				path : 'Werks',
				descending : false
			}
		}">
		<ColumnListItem vAlign="Middle" id="clif115">
			<cells>
				<ObjectIdentifier title="{Werks}" id="oif15"/>
                <Text text="{Name1}"  id="tf115"/>
			</cells>
		</ColumnListItem>
        <columns>
            <Column width="11rem" id="cf15">
				<header>
				    <Text text="Plant" id="lf115"/>
                </header>
			</Column>
			 <Column width="11rem" id="cf255">
				<header>
				    <Text text="Name" id="lf255"/>
                </header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>