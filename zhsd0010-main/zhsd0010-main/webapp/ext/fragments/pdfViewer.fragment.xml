<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Dialog id="idVH2" title="{i18n>PDFViewer}" contentHeight="auto" contentWidth="45%" draggable="true">
		<content>
		<ScrollContainer id="_IDGenScrollContainer1"
		height="100%"
		width="100%"
		horizontal="true"
		vertical="true">
		<FlexBox id="_IDGenFlexBox1" class="sapUiSmallMargin" direction="Column" renderType="Div">
			<FlexBox id="_IDGenFlexBox3" direction="Row" fitContainer="true" renderType="Div">
				<PDFViewer id="_IDGenPDFViewer2" class="sapUiSmallMarginBegin" source="{ui>/Source}" title="{/Title2}" height="{/Height}" width="auto">
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData2" growFactor="1"/>
					</layoutData>
				</PDFViewer>
			</FlexBox>
		</FlexBox>
	</ScrollContainer>
		</content>
        <buttons>
			<Button id="_IDGenButton1" text="{i18n>Close}" type="Reject" press="onClosePDF"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>